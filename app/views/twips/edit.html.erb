<% airbudd_form_for( :twip, @twip, :url => url_for( :controller => 'twips', :action => 'update'), :html => { :method => 'put', :class => 'yui-skin-sam'} ) do |f| %>
<%= render :partial => f %>
<% f.buttons do |b| %>
<%= b.button :save %>
<%= b.button :cancel, :url => url_for( :controller => 'twips', :action => 'show', :id => f.object ) %>
<% end %>
<% end %>